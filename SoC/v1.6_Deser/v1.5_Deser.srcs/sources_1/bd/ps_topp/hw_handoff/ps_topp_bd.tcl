  # Create instance: spectra_bram_ctrl, and set properties
  set spectra_bram_ctrl [ create_bd_cell -type ip -vlnv xilinx.com:ip:axi_bram_ctrl:4.0 spectra_bram_ctrl ]
  set_property -dict [ list \
