   CONFIG.NUM_MI {3} \
 ] $axi_interconnect_0

  # Create instance: oscillograms_bram_ctrl, and set properties
  set oscillograms_bram_ctrl [ create_bd_cell -type ip -vlnv xilinx.com:ip:axi_bram_ctrl:4.0 oscillograms_bram_ctrl ]
